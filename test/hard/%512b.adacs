-- $$512B -- adapted from the following ACVC test(s):
--   B52002B-AB.ADA
 
-- CHECK THAT THE LEFT SIDE OF AN ASSIGNMENT STMT MUST BE A VARIABLE.
 
-- CHECK THAT THE LHS MAY NOT BE AN ENUMERATION LITERIAL, BLOCK
--  IDENTIFIER, LOOP IDENTIFIER, PACKAGE NAME, TYPE NAME,
--  OR PRAGMA IDENTIFIER.
 
Pragma ListOn;
 
PACKAGE Main IS
 
     E1 : BOOLEAN;
 
BODY BEGIN
 
     TRUE := E1;          -- ERROR: ENUMERATION LITERAL ON LEFT.
     Main := MAIN;        -- ERROR: PACKAGE IDENTIFIER ON LEFT.
  B: BEGIN
          B := B;         -- ERROR: BLOCK IDENTIFIER ON LEFT.
     END B;
  F: LOOP
          F := F;         -- ERROR: LOOP IDENTIFIER ON LEFT.
     END LOOP F;
     ListOn := ListOn;    -- ERROR: PRAGMA IDENTIFER ON LEFT.
     REAL := 1.0;         -- ERROR: TYPE IDENTIFIER ON LEFT.
 
END Main;
