-- $$721C -- adapted from the following ACVC test(s):
--   C64002B-B.ADA
 
-- CHECK THAT PARAMETERLESS SUBPROGRAMS CAN BE CALLED WITH APPROPRIATE
--   NOTATION.
 
PACKAGE Main IS
 
     I    : INTEGER := 1;
 
BODY
 
     FUNCTION F0 RETURN INTEGER IS
     BEGIN
          RETURN 7;
     END F0;
 
     PROCEDURE P0 IS
     BEGIN
          I := 15;
     END P0;
 
BEGIN
 
     WRITE("Test $$721C:"; NEXT);
 
     IF (F0 /= 7) THEN
          WRITE(NEXT; "==> PARAMETERLESS FUNCTION CALL RETURNS BAD VALUE");
     END IF;
 
     P0;
     IF (I /= 15) THEN
          WRITE(NEXT; "==> PARAMETERLESS PROCEDURE CALL YIELDS INCORRECT RESULT");
     END IF;
 
     WRITE(NEXT);
 
END Main;
