-- $$713B -- adapted from the following ACVC test(s):
--   B58003A-B.ADA
 
-- CHECK THAT THE TYPE OF A RETURN STATEMENT'S EXPRESSION MUST MATCH
--    THE RETURN TYPE SPECIFIED IN THE FUNCTION'S SPECIFICATION.
 
PACKAGE Main IS
 
     I1 : INTEGER;
     B1 : BOOLEAN;
 
BODY
 
     FUNCTION FNC2 RETURN BOOLEAN IS
          B2 : CONSTANT BOOLEAN RANGE TRUE .. TRUE := TRUE;
     BEGIN
          RETURN B2;     -- LEGAL: TYPES MATCH, SUBTYPES OVERLAP.
     END FNC2;
 
     FUNCTION FNC1 RETURN INTEGER IS
     BEGIN
          RETURN 1.23;    -- ERROR: EXPRESSION TYPE DOES NOT MATCH.
     END FNC1;
 
BEGIN
     B1 := FNC2;
     I1 := FNC1;
END Main;
