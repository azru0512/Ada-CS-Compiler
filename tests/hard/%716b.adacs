-- $F011B -- adapted from the following ACVC test(s):
--   B63001A.ADA
 
-- CHECK THAT THE DESIGNATOR AT THE END OF A SUBPROGRAM BODY MUST BE THE
--   SAME AS THE DESIGNATOR USED IN THE SUBPROGRAM SPECIFICATION.
 
PACKAGE Main IS
  BODY
 
     PROCEDURE PROC1 IS
     BEGIN
          NULL;
     END PROC;           -- ERROR: DESIGNATOR DOES NOT MATCH.
 
     FUNCTION FUNC1 RETURN INTEGER IS
     BEGIN
          RETURN 1;
     END PROC1;          -- ERROR: DESIGNATOR DOES NOT MATCH.
 
     FUNCTION INTEGER RETURN BOOLEAN IS
     BEGIN
          RETURN TRUE;
     END REAL;           -- ERROR: DESIGNATOR DOES NOT MATCH.
 
     PROCEDURE PROC2 IS
     BEGIN
          NULL;
     END Main;           -- ERROR: DESIGNATOR DOES NOT MATCH.
 
BEGIN
 
     NULL;
 
END Main;
