-- $Q212B -- adapted from the following ACVC test(s):
--   B73006A-AB.ADA
 
-- CHECK THAT A PACKAGE BODY CANNOT BE PROVIDED UNLESS ITS SPECIFICATION
--    HAS ALREADY BEEN GIVEN.
 
PACKAGE BODY  PACK1  IS -- ERROR: BODY PRECEDES SPECIFICATION.
BEGIN
     NULL;
END  PACK1 ;
 
PACKAGE  PACK1  IS
   I : INTEGER ;
END  PACK1 ;
 
 
PACKAGE Main IS
BODY BEGIN
     NULL;
END Main;
